<div class="row">
  <div class="col-xs-6">
    #foreach($skillmatch in $skillmatches)
      <div class="thumbnail">
        <a href="users/$skillmatch.getId()"><h5>$skillmatch.getName()</h5></a>
        <ul>
          <li>$skillmatch.getSkills()</li>
          <li>$skillmatch.getLocation()</li>
        </ul>
      </div>
    #end
  </div>
